<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Login Page</title>
    <link rel="stylesheet" href="css/main.css">
    <link rel="stylesheet" href="css/styleslogin.css">
    <%- include('links') %>
  <body>
    <%- include('navbar'); %>

    <section>
      <div class="login-box">
        <form>
          <strong class="login-title">Please Login to Get Started</strong>
          <p>
            <label class="username">Username: </label>
            <input class="username-input" placeholder="Enter your username: "></input>
          </p>
          <p>
            <label class="password">Password: </label>
            <input class="password-input" placeholder="Enter your password: "></input>
            <a class="sign-up">Don't have an account? Sign up</a>
          </p>
          <p>
            <button class="login-button">Login</button>
          </p>

        </form>
      </div>
    </section>

    <%- include('file_ending'); %>
